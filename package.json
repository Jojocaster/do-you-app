    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "~29.7.0",
    "jest-coverage-badges": "1.1.2",
    "jest-expo": "~52.0.6",
    "jest-fetch-mock": "3.0.3",
    "prettier": "2.6.2",
    "react-test-renderer": "18.1.0",

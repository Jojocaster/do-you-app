{
  "name": "do-you-world",
  "version": "0.5.1",
  "main": "./AppEntry.js",
  "scripts": {
    "start": "expo start",
    "dev": "TARGET=dev expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "TZ=UTC jest --coverage --verbose",
    "test:watch": "TZ=UTC jest --watchAll --coverage --verbose",
    "test:badges": "jest-coverage-badges",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:prod": "eas build --platform android --profile production",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:prod": "eas build --platform ios --profile production",
    "publish:android": "eas submit -p android",
    "publish:ios": "eas submit -p ios"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jestSetup.js"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/lato": "0.2.2",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/material-top-tabs": "6.2.2",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@reduxjs/toolkit": "1.9.7",
    "@sentry/react-native": "~6.10.0",
    "@types/moment": "^2.13.0",
    "date-fns": "2.28.0",
    "date-fns-tz": "1.3.1",
    "expo": "52",
    "expo-asset": "~11.0.5",
    "expo-background-fetch": "~13.0.6",
    "expo-build-properties": "~0.13.3",
    "expo-cli": "^6.3.10",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-font": "~13.0.4",
    "expo-linking": "~7.0.5",
    "expo-localization": "~16.0.1",
    "expo-notifications": "~0.29.14",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-task-manager": "~12.0.6",
    "expo-web-browser": "~14.0.2",
    "moment": "^2.30.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-pager-view": "6.5.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-tab-view": "3.1.1",
    "react-native-track-player": "4.1.1",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "react-redux": "^8.0.0",
    "redux-persist": "6.0.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/config": "^10.0.0",
    "@testing-library/react-native": "9.1.0",
    "@types/jest": "28.1.6",
    "@types/react": "~18.3.12",
    "@types/react-dom": "~18.3.1",
    "@types/react-native": "0.68.2",
